{
	"name": "server---main",
	"version": "0.1.0",
	"repository": "https://github.com/pxseu/pxseu.com",
	"private": true,
	"main": "./dist/index.js",
	"scripts": {
		"compile": "rimraf dist && yarn lint && tsc --project tsconfig.server.json",
		"start": "node .",
		"dev": "cross-env NODE_ENV=development PORT=8080 nodemon -e ts --exec \"yarn compile && yarn start\"",
		"prod": "cross-env NODE_ENV=production PORT=6001 yarn start",
		"deploy": "yarn compile && yarn build",
		"lint": "eslint {src,server}/**/*.{ts,tsx} --fix",
		"build": "next build",
		"export": "next export",
		"postbuild": "next-sitemap"
	},
	"dependencies": {
		"cross-env": "^7.0.3",
		"express": "^4.17.1",
		"express-rate-limit": "^5.2.3",
		"helmet": "^4.1.1",
		"mongoose": "^5.11.13",
		"morgan": "^1.10.0",
		"next": "10.0.3",
		"next-pwa": "^5.0.3",
		"next-sitemap": "^1.4.5",
		"nprogress": "^0.2.0",
		"react": "16.14.0",
		"react-device-detect": "^1.14.0",
		"react-dom": "16.14.0",
		"react-particles-js": "^3.4.0",
		"twemoji": "^13.0.1",
		"webhook-discord": "^3.7.7"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/express-rate-limit": "^5.1.1",
		"@types/mongoose": "^5.10.1",
		"@types/morgan": "^1.9.2",
		"@types/node": "^14.14.17",
		"@types/node-fetch": "^2.5.7",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/twemoji": "^12.1.1",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"eslint": "^7.15.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-react": "^7.21.5",
		"nodemon": "2.0.7",
		"rimraf": "^3.0.2",
		"typescript": "^4.1.3"
	}
}
